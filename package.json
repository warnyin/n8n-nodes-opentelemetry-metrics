{
  "name": "@warnyin/n8n-nodes-opentelemetry-metrics",
  "version": "0.1.4",
  "description": "n8n community node for sending metrics via OpenTelemetry (OTLP)",
  "keywords": [
    "n8n-community-node-package",
    "opentelemetry",
    "metrics",
    "otlp"
  ],
  "license": "MIT",
  "author": {
    "name": "warnyin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warnyin/n8n-nodes-opentelemetry-metrics.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "node scripts/copy-assets.js",
    "dev": "tsc --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/OpenTelemetryMetrics/OpenTelemetryMetrics.node.js"
    ]
  },
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.50.0",
    "@opentelemetry/resources": "^1.8.0",
    "@opentelemetry/sdk-metrics": "^1.8.0",
    "n8n-nodes-opentelemetry-metrics": "^0.1.0"
  },
  "devDependencies": {
    "n8n-core": "*",
    "n8n-workflow": "*",
    "typescript": "^5.6.3"
  }
}
